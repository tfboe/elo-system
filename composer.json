{
  "name": "laravel/lumen",
  "description": "The Laravel Lumen Framework.",
  "keywords": [
    "framework",
    "laravel",
    "lumen"
  ],
  "authors": [
    {
      "name": "Benedikt Klocker",
      "email": "blacktemplar@a1.net"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/blacktemplar/jwt-auth-guard"
    }
  ],
  "license": "GPL-3.0-or-later",
  "type": "project",
  "require": {
    "php": ">=7.1",
    "vlucas/phpdotenv": "3.6.0",
    "tfboe/fm-lib": "dev-master",
    "palanik/lumen-cors": "0.0.3",
    "beberlei/doctrineextensions": "^1.0"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "2.6.6",
    "fzaninotto/faker": "1.9.1",
    "laravel-doctrine/migrations": "1.3.1",
    "phpunit/phpcov": "6.0.1",
    "phpunit/phpunit": "8.5.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "classmap": [
      "database/"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "platform": {
      "php": "7.2"
    }
  }
}
