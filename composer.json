{
  "name": "laravel/lumen",
  "description": "The Laravel Lumen Framework.",
  "keywords": [
    "framework",
    "laravel",
    "lumen"
  ],
  "authors": [
    {
      "name": "Benedikt Klocker",
      "email": "blacktemplar@a1.net"
    }
  ],
  "license": "GPL-3.0-or-later",
  "type": "project",
  "require": {
    "php": ">=7.1",
    "laravel/lumen-framework": "5.5.*",
    "vlucas/phpdotenv": "~2.2",
    "tymon/jwt-auth": "^1.0.0",
    "irazasyed/jwt-auth-guard": "dev-master",
    "laravel-doctrine/orm": "^1.2.12",
    "laravel-doctrine/extensions": "^1.0.12",
    "gedmo/doctrine-extensions": "^2.4.30",
    "tfboe/fm-lib": "0.1.36",
    "palanik/lumen-cors": "dev-master",
    "beberlei/DoctrineExtensions": "^1.0"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "~6.0",
    "mockery/mockery": "~0.9",
    "barryvdh/laravel-ide-helper": "^2.4",
    "laravel-doctrine/migrations": "^1.0.0",
    "phpunit/phpcov": "^4.0",
    "phpmd/phpmd": "^2.6",
    "pdepend/pdepend": "^2.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "classmap": [
      "database/"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true
  }
}
