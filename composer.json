{
  "name": "laravel/lumen",
  "description": "The Laravel Lumen Framework.",
  "keywords": [
    "framework",
    "laravel",
    "lumen"
  ],
  "authors": [
    {
      "name": "Benedikt Klocker",
      "email": "blacktemplar@a1.net"
    }
  ],
  "repositories": [
      {
          "type": "vcs",
          "url": "https://github.com/blacktemplar/jwt-auth-guard"
      }
  ],
  "license": "GPL-3.0-or-later",
  "type": "project",
  "require": {
    "php": ">=8.2",
    "ext-mbstring": "*",
    "laravel/lumen-framework": "^8.1",
    "vlucas/phpdotenv": "^5.2",
    "tymon/jwt-auth": "^1.0.0",
    "irazasyed/jwt-auth-guard": "dev-master",
    "laravel-doctrine/orm": "~1.7",
    "laravel-doctrine/extensions": "~1.4",
    "gedmo/doctrine-extensions": "^3.0",
    "tfboe/fm-lib": "dev-elo-system-old",
    "palanik/lumen-cors": "dev-master",
    "beberlei/doctrineextensions": "^1.0",
    "flipbox/lumen-generator": "^8.2"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "~9.0",
    "mockery/mockery": "~0.9",
    "barryvdh/laravel-ide-helper": "^2.4",
    "laravel-doctrine/migrations": "^3.0.0",
    "phpunit/phpcov": "^8.0",
    "phpmd/phpmd": "^2.6",
    "pdepend/pdepend": "^2.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "classmap": [
      "database/"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true
  }
}
